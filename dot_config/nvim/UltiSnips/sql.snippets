snippet tr "Truncate table" b
TRUNCATE TABLE ${1: table name};
$0
endsnippet

snippet dr "Drop table" b
DROP TABLE ${1: table name};
$0
endsnippet

snippet cr "Create new table" b
CREATE TABLE $1 (
	$0
);
endsnippet

snippet var "New varchar" b
${1: column name} VARCHAR ${2:UNIQUE} ${3:NOT NULL} ${4:PRIMARY KEY},
$0
endsnippet
